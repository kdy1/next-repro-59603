{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "dts": true,
      "tsx": true,
      "decorators": true
    },
    "transform": {
      "decoratorVersion": "2022-03",
      "react": {
        "runtime": "automatic"
      },
    },
    "target": "es5",
    "baseUrl": "src",
    "paths": {
      "@": ["."],
      "@/*": ["*"]
    },
    "externalHelpers": true
  },
  "sourceMaps": "inline",
  
}
